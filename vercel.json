{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/news", "dest": "/index.html" },
    { "src": "/store", "dest": "/index.html" },
    { "src": "/rules", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
