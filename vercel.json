{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/((?!assets/|favicon\\.ico|.*\\..*).*)",
      "dest": "/index.html"
    }
  ]
}
